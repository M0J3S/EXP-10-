package experiments;
class Employee
{
public String name=null;
public int number=0;
public void data_e(String ename,int eno)
{
name=ename;
number=eno;
}
public void display_e()
{
System.out.println("EMPLOYE NAME : "+name+"\nEMPLOYEE NUMBER : "+number);
}
}
class Department extends Employee
{
public String d_name = null;
public int dnumber = 0;
public void data_d(String dname,int dno)
{
d_name=dname;
dnumber=dno;
}
public void display_d()
{
System.out.println("DEPARTMENT NAME : "+d_name+"\nDEPARTMENT NUMBER :
"+dnumber);
}
}
public class Neeraj_mojes extends Thread
{
public static void main(String args[])
{
Department obj=new Department();
obj.data_e("NEERAJ",393);
obj.display_e();
obj.data_d("ACCOUNTS MANEGMENT",12)
obj.display_d();
}
}
